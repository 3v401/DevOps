{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "K8sELBPermissions",
			"Effect": "Allow",
			"Action": [
				"ec2:Describe*",
				"ec2:CreateTags",
				"ec2:AuthorizeSecurityGroupIngress",
				"elasticloadbalancing:*",
				"iam:ListServerCertificates",
				"iam:GetServerCertificate",
				"iam:ListRolePolicies",
				"iam:GetRolePolicy"
			],
			"Resource": "*"
		}
	]
}
